<h3>New User</h3>


<%= form_for @user do |f| %>
  <%= render 'shared/errors', obj: @user %>
<div class='well'>
  <div class='control-group'>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  
  <div class='control-group'>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
    <%= f.submit "Register", class: "btn btn-primary" %>
  </div>
<% end %>
